package com.test.zoo;

public class ZooClass {
    
    Scanner scanner;
    
    // 동물객체들을 담을 배열
    AnimalClass [] aninalArray;
    // 동물의 수
    int animalCount;
    
    public ZooClass(Scanner scanner) {
        this.scanner = scanner;
        aninalArray = new AnimalClass[1000];
        animalCount = 0;
    }
}
