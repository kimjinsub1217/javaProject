package com.test.main;

public class MainClass {

	public static void main(String[] args) {
		// 정수형 기억장소 10개를 관리하는 배열
		int[] array1 = new int[10];

		// 값을 담는다.
		array1[1] = 100;
		array1[2] = 200;
		array1[3] = 300;
		
		//사용한다.
		System.out.printf("array[0] : %d\n",array1[0]);
		System.out.printf("array[1] : %d\n",array1[1]);
		System.out.printf("array[2] : %d\n",array1[2]);
	}

}
